{
  "name": "ng8-ivy",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "cd server && node start-every-server.js",
    "poststart": "npm run start:product",
    "build": "npm run build:root && npm run build:product",
    "build:root": "ng build --prod",
    "postbuild:root": "cpx dist/ng8-ivy/** server/static",
    "build:product": "ng build product --prod --single-bundle  --outputHashing=none",
    "build:product2": "ng build product --prod --single-bundle  --outputHashing=none --extraWebpackConfig webpack.extra.js",
    "postbuild:product": "cpx dist/product/main*.js projects/product/server/static",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "install": "npm run install:root && npm run install:common",
    "install:root": "cd server && npm install",
    "install:common": "cd projects/common && npm install",
    "start:root": "cd server && node index.js",
    "start:product": "cd projects/product/server && node index.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0-rc.4",
    "@angular/common": "~8.0.0-rc.4",
    "@angular/compiler": "~8.0.0-rc.4",
    "@angular/core": "~8.0.0-rc.4",
    "@angular/elements": "^7.2.15",
    "@angular/forms": "~8.0.0-rc.4",
    "@angular/platform-browser": "~8.0.0-rc.4",
    "@angular/platform-browser-dynamic": "~8.0.0-rc.4",
    "@angular/router": "~8.0.0-rc.4",
    "socket.io-client": "^2.2.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1",
    "document-register-element": "^1.7.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0-rc.4",
    "@angular/cli": "~8.0.0-rc.4",
    "@angular/compiler-cli": "~8.0.0-rc.4",
    "@angular/language-service": "~8.0.0-rc.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ngx-build-plus": "^8.0.0-rc.3.0.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
